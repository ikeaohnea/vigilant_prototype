CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,MAAA,CAAA,SAAA,CAAS,MAAT,EAAkB,MAAlB,CAAF;AAAA,KAAA,CAI5B;AAJyD,IAAA,GAAG,EAAC;AAI7D,GAJT,E,oBACI,YAAA,CAES,SAFT,EAES,IAFT,EAES,WAAA,CAFW,MAAA,CAAA,WAEX,EAFsB,UAAhB,CAAgB,EAAf;yBAAhB,YAAA,CAES,QAFT,EAES;AAFyB,MAAA,GAAG,EAAE,CAE9B;AAFkC,MAAA,KAAK,EAAE;AAEzC,KAFT,E,iBACO,C,CADP,EACQ;AAAA;AADR,MACQ,CAAA,OAAA,CADR,C;GAES,CAFT,E;;AAAA,G,EADJ,E;;AAAA,GAWE,EANF,YAAA,CAIS,QAJT,EAIS;AAJD,IAAA,KAAK,EAAC,eAIL;AAJsB,IAAA,QAAM,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,MAAA,CAAA,SAAA,CAAS,MAAT,EAAkB,MAAlB,CAAF;AAAA,KAAA,CAI5B;AAJyD,IAAA,GAAG,EAAC;AAI7D,GAJT,E,oBACI,YAAA,CAES,SAFT,EAES,IAFT,EAES,WAAA,CAFW,MAAA,CAAA,aAEX,EAFwB,UAAlB,CAAkB,EAAjB;yBAAhB,YAAA,CAES,QAFT,EAES;AAF2B,MAAA,GAAG,EAAE,CAEhC;AAFoC,MAAA,KAAK,EAAE,MAAA,CAAA,YAAA,IAAgB,CAAC,GAAA,CAAjB;AAE3C,KAFT,E,iBACO,MAAA,CAAA,YAAA,IAAgB,CAAC,GAAA,CAAjB,C,CADP,EACwB;AAAA;AADxB,MACwB,CAAA,OAAA,CADxB,C;GAES,CAFT,E;;AAAA,G,EADJ,E;;AAAA,GAME,EADF,YAAA,CAA0D,QAA1D,EAA0D;AAAlD,IAAA,KAAK,EAAC,cAA4C;AAA5B,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,MAAA,CAAA,KAAA,OAAA,MAAA,YAAF;AAAA,KAAA;AAAuB,GAA1D,EAA4C,OAA5C,CACE,CAjBN,C","sourcesContent":["<template>\n<div class=\"date-picker\">\n    <select class=\"custom-select\" @change=\"editValue('months', $event)\" ref=\"month\">\n        <option v-for='(month, index) in months' :value=\"index\" :key='month'>\n            {{ month }}\n        </option>\n    </select>\n    <select class=\"custom-select\" @change=\"editValue('date', $event)\" ref=\"date\">\n        <option v-for=\"i in daysInMonth\" :key=\"i\" :value=\"i\">\n            {{ i }}\n        </option>\n    </select>\n    <select class=\"custom-select\" @change=\"editValue('year', $event)\" ref=\"year\">\n        <option v-for=\"i in numberOfYears\" :key=\"i\" :value=\"startingYear + (i - 1)\">\n            {{ startingYear + (i - 1)}}\n        </option>\n    </select>\n    <button class=\"reset-button\" @click=\"today\">Today</button>\n</div>\n</template>\n\n<script>\nimport {ref, onMounted} from 'vue'\n\nimport moment from 'moment'\n\nexport default {\n    setup (props, context) {\n        const dateValue = moment()\n        const daysInMonth = ref(dateValue.daysInMonth())\n\n        const month = ref(null)\n        const date = ref(null)\n        const year = ref(null)\n\n        const updateElements = () => {\n            month.value.value = dateValue.month()\n            date.value.value = dateValue.date()\n            year.value.value = dateValue.format('YYYY')\n            context.emit('update', dateValue)\n        }\n\n        const editValue = (unit, evt) => {\n            dateValue.set(unit, evt.target.value)\n            daysInMonth.value = dateValue.daysInMonth()\n            updateElements()\n        }\n\n        const today = () => {\n            const now = moment()\n            dateValue.year(now.year())\n            dateValue.month(now.month())\n            dateValue.date(now.date())\n            updateElements()\n        }\n\n        onMounted(() => {\n            updateElements()\n        })\n\n        const months = ['January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October', 'November', 'December']\n        const startingYear = 2010\n        const numberOfYears = 20\n\n        return {\n            month,\n            date,\n            year,\n            editValue,\n            daysInMonth,\n            today,\n            months,\n            numberOfYears,\n            startingYear\n        }\n    }\n}\n</script>\n\n<style scoped>\n.date-picker {\n    display: flex;\n    margin-left: 10px;\n    margin-right: 10px;\n}\n\n.custom-select {\n    background-color: #eee;\n    float: left;\n    position: relative;\n    border: none;\n    color: #333;\n    display: block;\n    font-size: 16px;\n    height: 32px;\n    padding: 5px 30px 5px 10px;\n    outline: none;\n    margin-right: 5px;\n}\n\n.reset-button {\n    background-color: red;\n    border: none;\n    outline: none;\n}\n\n.custom-select:after {\n    content: '\\25bc';\n    color: #aaa;\n    font-size: 12px;\n    position: absolute;\n    right: 8px;\n    top: 10px;\n}\n</style>"],"sourceRoot":""}]}